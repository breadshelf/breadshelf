

<h1>Events</h1>

<h2>Total Events: <%= @total_events %></h2>

<h3>Sign ups: <%= @sign_up_count %></h3>
<h3>Sign ins: <%= @sign_in_count %></h3>

<h3>Daily Active Users (DAUs):</h3>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Active Users</th>
    </tr>
  </thead>
  <tbody>
    <% @daus.each do |dau| %>
      <tr>
        <td><%= dau[:date] %></td>
        <td><%= dau[:count] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h3>Page Views:</h3>
<ul>
    <% @page_view_counts.each do |page, count| %>
        <li><%= page %> - <%= count %></li>
    <% end %>
</ul>

